{
  "name": "lv-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "watchify --poll=3000 -d client/src/app/app.js -v -o client/dist/app/bundle.js",
    "build-prod": "browserify client/src/app/app.js -t [envify --NODE_ENV production] | uglifyjs -cm > client/dist/app/bundle.js",
    "build-dev": "browserify client/src/app/app.js --debug -t [envify --NODE_ENV development] -o client/dist/app/bundle.js"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.13.2",
    "bootstrap": "^3",
    "browserify": "^12.0.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "envify": "^3.4.1",
    "express": "~4.13.1",
    "flux": "^2.0.1",
    "history": "^3.0.0",
    "humps": "^1.1.0",
    "immutable": "^3.8.1",
    "jquery": "^2.2",
    "keymirror": "~0.1.0",
    "morgan": "~1.6.1",
    "normalizr": "^2.1.0",
    "nuka-carousel": "^2.0.2",
    "react": "^15.1.0",
    "react-bootstrap": "^0.30.1",
    "react-cookie": "^0.4.6",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "regenerator-runtime": "^0.9.5",
    "serve-favicon": "~2.3.0",
    "sweetalert-react": "*",
    "uglify-js": "~2.4.15"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react",
            "stage-1"
          ]
        }
      ]
    ]
  }
}
