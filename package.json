{
  "name": "lv-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "bundle": "watchify -o public/javascripts/bundle.js -v -d public/javascripts/app.js  > bundleoutput.txt",
    "build": "browserify public/javascripts/app.js -t [babelify --presets [ es2015 ], envify --NODE_ENV production] | uglifyjs -cm > public/javascripts/bundle.js",
    "boot-docker": "docker run -d -p 3000:3000 -v process.env.PATH:/src lv-client",
    "build-docker": "docker build -t lv-client ."
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.12.0",
    "react-cookie": "^0.2.6"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
