{
  "name": "lv-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "bundle": "watchify --poll=3000 -d public/javascripts/app.js -v -o \"uglifyjs -cm > public/javascripts/bundle.js\" -t [babelify --presets [es2015,react]]",
    "bundle-dev": "watchify --poll=3000 -d public/javascripts/app.js -v -o public/javascripts/bundle.js -t [babelify --presets [es2015,react]]",
    "build": "browserify public/javascripts/app.js -t [babelify --presets [ es2015 , react], envify --NODE_ENV production] | uglifyjs -cm > public/javascripts/bundle.js",
    "build-dev": "browserify public/javascripts/app.js --debug -t [babelify --presets [ es2015 , react], envify --NODE_ENV production] -o public/javascripts/bundle.js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "classnames": "^2.1.3",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "history": "^3.0.0",
    "react-router": "^2.4.1",
    "morgan": "~1.6.1",
    "react-cookie": "^0.4.6",
    "serve-favicon": "~2.3.0"

  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^12.0.1",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "install": "^0.8.1",
    "npm": "^3.9.5",
    "uglify-js": "~2.4.15",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
