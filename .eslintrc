{
  "extends": [
    "google",
    "plugin:react/recommended"
  ],
  "globals": {
    "fetch": true,
    "window": true,
    "document": true
  },
  "rules": {
    "quotes": 1,
    "max-len": [1, 120],
    "operator-linebreak": [1, "after"],
    "no-loop-func": [0]
  },
  "plugins": [
    "react"
  ],
  "parser-options": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}